<ons-page style="margin-top: 0px;">
	<ons-list modifier="inset"">
	  {{#each data.notification}}
	  <ons-list-item style="border-bottom: 1px solid #000; min-height: 30px; padding: 15px; background-color: #f9f9f9; color: #000;" class="list-not" onclick="getToCasoByNotification('{{caso}}');" tappable>{{title}}</ons-list-item>
	  {{/each}}
	</ons-list>
</ons-page>

<script type="text/javascript">
    function getToCasoByNotification(caso){
	document.getElementById('tabbar').setActiveTab(1);
	setTimeout(function(){
		$("#link-caso-view-"+caso).trigger( "click" );
	},100);
    }
</script>